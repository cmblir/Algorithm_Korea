def solution(nums):
    n = len(nums)
    result = 0
    for i in range(n):
        for j in range(i + 1, n):
            for h in range(j + 1, n):
                error = 0
                sums = nums[i] + nums[j] + nums[h]
                for k in range(1, sums + 1):
                    if sums % k == 0:
                        error += 1
                if error == 2:
                    result += 1
    return result
                